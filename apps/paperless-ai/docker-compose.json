{
  "$schema": "../dynamic-compose-schema.json",
  "services": [
    {
      "name": "paperless-ai",
      "image": "clusterzx/paperless-ai:latest",
      "isMain": true,
      "internalPort": 8000,
      "environment": {
        "PAPERLESS_AI_PORT": "${APP_PORT}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/app/data"
        }
      ]
    }
  ]
}
