{
  "services": [
    {
      "name": "paperless-ai",
      "image": "clusterzx/paperless-ai:latest",
      "isMain": true,
      "internalPort": 3000,
      "env": {
        "PAPERLESS_URL": "${PAPERLESS_URL}",
        "PAPERLESS_API_KEY": "${PAPERLESS_API_KEY}",
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",      // oder OLLAMA_URL
        "PORT": "3000"
      },
      "depends_on": ["paperless"]        // wenn Paperless-ngx ebenfalls in Runtipi l√§uft
    }
  ]
}
